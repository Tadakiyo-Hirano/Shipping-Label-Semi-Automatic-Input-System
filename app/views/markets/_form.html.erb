<%= form_with(model: @market, local: true) do |f| %>
  <%= f.label :name, class: "label-new" %>
  <%= f.text_field :name, class: "form-control" %>

  <%= f.label :code, class: "label-new" %>
  <%= f.text_field :code, class: "form-control" %>

  <%= f.submit "#{yield(:button_text)}", class: "btn btn-secondary btn-block btn-new"  %>
<% end %>
